<?xml version='1.0'?>

<robot name="trackbot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find gcamp_gazebo)/urdf/trackbot.gazebo" />

  <link name="base_footprint"></link>

  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="base_footprint"/>
    <child link="base_link" />
  </joint>

  <link name='base_link'>
    <pose frame=''>0 0 0 0 -0 0</pose>

    <inertial>
      <origin xyz="-0.122 0 0.118" rpy="1.5708 -0 0" />
      <mass value="13.14" />
      <inertia 
        ixx="0.10019" ixy="0.0" ixz="0.0" 
        iyy="0.345043" iyz="0.0" 
        izz="0.302044" 
      />
    </inertial>

    <collision name='base_link_collision'>
