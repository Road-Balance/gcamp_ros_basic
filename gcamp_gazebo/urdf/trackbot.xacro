      <geometry>
        <box size="0.50017 0.24093 0.139" />
      </geometry>
    </collision>

    <visual name='base_link_visual'>
      <origin xyz="-0.122 0 0.118" rpy="0 -0 0" />
      <geometry>
        <box size="0.50017 0.24093 0.139" />
      </geometry>
    </visual>
  </link>

    
